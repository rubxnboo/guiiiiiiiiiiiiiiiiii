local player = game.Players.LocalPlayer local gui = Instance.new("ScreenGui") gui.Name = "RUBXN_BOO_GUI" gui.ResetOnSpawn = false gui.Parent = player:WaitForChild("PlayerGui") -- Frame principal local main = Instance.new("Frame") main.Parent = gui main.Size = UDim2.new(0, 420, 0, 240) main.Position = UDim2.new(0.5, -210, 0.5, -120) main.BackgroundColor3 = Color3.fromRGB(20, 20, 25) main.BorderSizePixel = 0 -- Esquinas redondeadas local corner = Instance.new("UICorner") corner.CornerRadius = UDim.new(0, 22) corner.Parent = main -- Degradado local gradient = Instance.new("UIGradient") gradient.Color = ColorSequence.new{ ColorSequenceKeypoint.new(0, Color3.fromRGB(90, 0, 140)), ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 170, 255)) } gradient.Rotation = 35 gradient.Parent = main -- Título local title = Instance.new("TextLabel") title.Parent = main title.Size = UDim2.new(1, 0, 0, 90) title.BackgroundTransparency = 1 title.Text = "RUBXN_BOO" title.Font = Enum.Font.GothamBlack title.TextSize = 48 title.TextColor3 = Color3.fromRGB(255, 255, 255) title.TextStrokeTransparency = 0.6 -- Subtexto decorativo local subtitle = Instance.new("TextLabel") subtitle.Parent = main subtitle.Position = UDim2.new(0, 0, 0, 85) subtitle.Size = UDim2.new(1, 0, 0, 30) subtitle.BackgroundTransparency = 1 subtitle.Text = "Premium Interface" subtitle.Font = Enum.Font.Gotham subtitle.TextSize = 16 subtitle.TextColor3 = Color3.fromRGB(220, 220, 220) -- Botón Encender local button = Instance.new("TextButton") button.Parent = main button.Size = UDim2.new(0, 220, 0, 55) button.Position = UDim2.new(0.5, -110, 0.7, 0) button.Text = "ENCENDER" button.Font = Enum.Font.GothamBold button.TextSize = 22 button.TextColor3 = Color3.fromRGB(255, 255, 255) button.BackgroundColor3 = Color3.fromRGB(15, 15, 20) button.BorderSizePixel = 0 local buttonCorner = Instance.new("UICorner") buttonCorner.CornerRadius = UDim.new(0, 16) buttonCorner.Parent = button -- Hover efecto button.MouseEnter:Connect(function() button.BackgroundColor3 = Color3.fromRGB(35, 35, 45) end) button.MouseLeave:Connect(function() button.BackgroundColor3 = Color3.fromRGB(15, 15, 20) end) -- Acción del botón button.MouseButton1Click:Connect(function() gui:Destroy() loadstring(game:HttpGet("https://raw.githubusercontent.com/prostone55/NitoHub/refs/heads/main/FlickFPS.lua"))() end)
